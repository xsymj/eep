                            <!--<input class="ui-input" ms-value="el.remark" ms-attr-id="id8{{$index}}"maxlength="30" ms-duplex="el.remark" />-->
                        <!--</td>-->
  						<td class="tx">
                            <input class="ui-input" ms-value="el.remark" ms-attr-id="id8{{$index}}"maxlength="30" ms-duplex="el.remark" />
                        </td>
  					</tr>
  				</tbody>
  				<tfoot>
  					<tr>
  						<td ms-attr-colspan="(type==4?7:6)" align="center"><b>数量合计：</b></td>
  						<td align="center" colspan="8">{{(sumAmount)}}</td>
  					</tr>
  				</tfoot>

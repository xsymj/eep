  						<th width="167px" class="tx">商品名称</th>
  						<th width="167px" class="tx">品牌</th>
  						<th width="167px" class="tx">型号</th>
  						<th width="167px" class="tx">规格</th>
  						<th width="70px" class="tx">计量单位</th>
  						<th width="167px" class="tx">数量</th>
